#! /bin/sh
BoardID=$(cfecmd -g BoardID)
BoardID=${BoardID:36}

SUPPORT_MOCA_WAN=0
SUPPORT_MOCA_LAN=0
SUPPORT_SFP_WAN=0
SUPPORT_DSL_WAN=0
SUPPORT_VOIP=0
SUPPORT_DAUTHER_CARD=0
SUPPORT_PCA9555=0

SUPPORT_3D=0
SINGLE_USB=0
SINGLE_SFP_LED=0
SPECAL_LED_INDEX=0
#DUAL_ATM=1 is means: support two ATM interface: atm0(PVC=0/0/33, atm1(PVC=0/8/35))
DUAL_ATM=0
# project = TELUS CENTURYLINK BHR5C ...
#PROJECT="TELUS"

if [ "${BoardID}" = "T3200" ]; then
  SINGLE_USB=1
  SUPPORT_DSL_WAN=1
  SUPPORT_DAUTHER_CARD=1
  DUAL_ATM=1
elif [ "${BoardID}" = "T3200B" ]; then
  SUPPORT_DSL_WAN=1
  SUPPORT_SFP_WAN=1
  SUPPORT_DAUTHER_CARD=1
  SUPPORT_PCA9555=1
  SUPPORT_3D=1
  DUAL_ATM=1
elif [ "${BoardID}" = "T3200M" ]; then
  SINGLE_USB=1
  SUPPORT_DSL_WAN=1
  SUPPORT_SFP_WAN=1
  SUPPORT_MOCA_LAN=1
  SUPPORT_DAUTHER_CARD=1
  DUAL_ATM=1
elif [ "${BoardID}" = "T3200R" ]; then
  SINGLE_USB=1
  SUPPORT_MOCA_LAN=1
elif [ "${BoardID}" = "T3200BM" ]; then
  SUPPORT_DSL_WAN=1
  SUPPORT_SFP_WAN=1
  SUPPORT_MOCA_LAN=1
  SUPPORT_DAUTHER_CARD=1
  SUPPORT_PCA9555=1
  SUPPORT_3D=1
  DUAL_ATM=1
elif [ "${BoardID}" = "T3200BV" ]; then
  SUPPORT_DSL_WAN=1
  SUPPORT_SFP_WAN=1
  SUPPORT_VOIP=1
  SUPPORT_DAUTHER_CARD=1
  SUPPORT_PCA9555=1
  SUPPORT_3D=1
  DUAL_ATM=1
elif [ "${BoardID}" = "C3000" ]; then
  SUPPORT_DSL_WAN=1
  SUPPORT_SFP_WAN=1
  SUPPORT_VOIP=1
  SUPPORT_DAUTHER_CARD=1
  SUPPORT_PCA9555=1
  SUPPORT_3D=1
  SPECAL_LED_INDEX=1
elif [ "${BoardID}" = "T3200BMV" ]; then
  SUPPORT_MOCA_LAN=1
  SUPPORT_SFP_WAN=1
  SUPPORT_DSL_WAN=1
  SUPPORT_VOIP=1
  SUPPORT_DAUTHER_CARD=1
  SUPPORT_PCA9555=1
  SUPPORT_3D=1
  DUAL_ATM=1
elif [ "${BoardID}" = "T3200MV" ]; then
  SUPPORT_MOCA_LAN=1
  SUPPORT_DSL_WAN=1
  SUPPORT_VOIP=1
  SUPPORT_DAUTHER_CARD=1
  SUPPORT_PCA9555=1
  DUAL_ATM=1
elif [ "${BoardID}" = "T3250" ]; then
  SINGLE_USB=1
  SUPPORT_DSL_WAN=1
  DUAL_ATM=1
elif [ "${BoardID}" = "T3250V" ]; then
  SINGLE_USB=1
  SUPPORT_DSL_WAN=1
  SUPPORT_VOIP=1
  DUAL_ATM=1
elif [ "${BoardID}" = "T3255" ]; then
  SINGLE_USB=1
  SUPPORT_DSL_WAN=1
  DUAL_ATM=1
elif [ "${BoardID}" = "T3255V" ]; then
  SINGLE_USB=1
  SUPPORT_DSL_WAN=1
  SUPPORT_VOIP=1
  DUAL_ATM=1
fi
